    <!-- ======= Calender and notification stuff ======= -->
    <section class="info-box py-0">
      <div class="container-fluid">

        <div class="row">

          <div class="col-sm-12">


            <div class="accordion-list">
              <ul id="news_ki_list">
                <% 
                function date_sort(a, b) {
                  return new Date(b.date_created).getTime() - new Date(a.date_created).getTime();
              }
              data.sort(date_sort);
                for( var index = 1; index <= data.length ; index++ ) { %>
                  <li style="box-shadow: 0 2px 4px 0 rgba(0, 0, 0, 0.2), 0 3px 10px 0 rgba(0, 0, 0, 0.19);">

                    <a data-toggle="collapse" class="collapse" href="#accordion-list-<%= index %>">
                      <div class="row">
                        <div class="col-11"> <span><%= index %> </span> <%= data[index-1].toJSON()['title'] %> </div>
                        
                        <div id="notif-date" class="col-1" style="font-size: small;"><%= data[index-1].toJSON()['date_created'].getDate() %>/<%= data[index-1].toJSON()['date_created'].getMonth()+1 %>/<%= data[index-1].toJSON()['date_created'].getFullYear() %> </div>
                      </div>
                      
                      
                      <i class="bx bx-chevron-down icon-close"></i>
                      <i class="bx bx-chevron-up icon-show"></i>
                    </a>
  
                    <div id="accordion-list-<%= index %>" class="collapse" data-parent=".accordion-list">
                      <p><%= data[index-1].toJSON()['content'] %> </p>
                    </div>
  
                  </li>
                <% } %>
              </ul>
            </div>

          </div>
        </div>

      </div>
    </section><!-- End Info Box Section -->

